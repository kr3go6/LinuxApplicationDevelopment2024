#: bin_search.c:94
#, c-format
msgid "\t-h:\tprint this help\n"
msgstr "\t-h:\tвывести на экран эту помощь\n"

#: bin_search.c:95
#, c-format
msgid "\t--help\n"
msgstr "\t--help\n"

#: bin_search.c:96
#, c-format
msgid "\t-r:\troman mode\n"
msgstr "\t-r:\tиспользовать римские числа\n"

#: bin_search.c:111
#, c-format
msgid "is your number greater than %s? [yes/no]\n"
msgstr "ваше число больше, чем %s? [да/нет]\n"

#: bin_search.c:121
msgid "yes"
msgstr "да"

#: bin_search.c:124
msgid "no"
msgstr "нет"

#: bin_search.c:128
#, c-format
